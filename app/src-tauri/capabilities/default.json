{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-execute",
    "store:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [{ "name": "solaris_core", "sidecar": true }]
    }
  ]
}
